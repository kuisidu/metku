k0[0,0] = rodc
k0[3,3] = rodc
k0[0,3] = -rodc
k0[3,0] = k0[0,3]
#
k0[1,1] = 12*EI1/Le^3
k0[4,4] = k0[1,1]
k0[1,4] = -k0[1,1]
k0[4,1] = k0[1,4]
#
k0[1,2] = 6*EI1/Le^2
k0[2,1] = k0[1,2]
k0[1,5] = k0[1,2]
k0[5,1] = k0[1,5]
#
k0[2,4] = -k0[1,2]
k0[4,2] = k0[2,4]
#
k0[4,5] = k0[4,2]
k0[5,4] = k0[4,5]
#
k0[2,5] = 2*EI1/Le
k0[5,2] = k0[2,5]
#
k0[2,2] = 4*EI1/Le
k0[5,5] = k0[2,2]
